						"exec": [
							"pm.test('Status code is 201 Created', function () {\r",
							"    pm.response.to.have.status(201);\r",
							"    pm.response.to.have.status('Created');\r",
							"});\r",
							"\r",
							"pm.test('Location is present', function () {\r",
							"    pm.response.to.have.header('Location');\r",
							"    \r",
							"    \r",
							"    pm.expect(pm.response.headers.get('Location')).to.include(\r",
							"        `http://localhost:3009/fit-and-competent-api/v1/applications/`\r",
							"    );\r",
							"});\r",
							"\r",
							"pm.collectionVariables.set(\"FC_NEW_APP_ID\", pm.response.headers.get('Location').split('/').pop());\r",
